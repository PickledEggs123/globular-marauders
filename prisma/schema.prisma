generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model planets {
  id        Int    @id @default(autoincrement())
  seed      String @db.VarChar(255)
  meshUrl   String @db.VarChar(255)
  meshesUrl String @db.VarChar(255)
}
